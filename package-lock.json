    "packages/shared/data": {
      "name": "@gato/data",
      "version": "0.1.0",
      "extraneous": true
    },
    "packages/shared/gatoapp": {
      "version": "0.1.0"
    },
    "packages/shared/public": {
      "name": "@gato/public",
      "version": "0.1.0",
      "extraneous": true
    },
    "packages/shared/types": {
      "name": "@gato/types",
      "version": "0.1.0",
      "extraneous": true
    },
    "packages/shared/utils": {
      "name": "@gato/utils",
      "version": "0.1.0",
      "extraneous": true
    }
  }
