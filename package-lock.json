{
  "name": "gatowebsites",
  "version": "3.0.0",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "name": "gatowebsites",
      "version": "3.0.0",
      "workspaces": [
        "apps/*",
        "packages/config/*",
        "packages/shared/*"
      ]
    },
    "apps/gatographql": {
      "version": "0.1.0",
      "dependencies": {
        "gatoapp": "*"
      }
    },
    "apps/gatoplugins": {
      "version": "0.1.0",
      "dependencies": {
        "gatoapp": "*"
      }
    },
    "node_modules/gatoapp": {
      "resolved": "packages/shared/gatoapp",
      "link": true
    },
    "node_modules/gatographql": {
      "resolved": "apps/gatographql",
      "link": true
    },
    "node_modules/gatoplugins": {
      "resolved": "apps/gatoplugins",
      "link": true
    },
    "packages/shared/app": {
      "name": "@gato/app",
      "version": "0.1.0",
      "extraneous": true
    },
    "packages/shared/components": {
      "name": "@gato/components",
      "version": "0.1.0",
      "extraneous": true,
      "dependencies": {
        "@headlessui/react": "^2.1.9",
        "@heroicons/react": "^2.1.5",
        "@sindresorhus/slugify": "^2.2.1",
        "@types/node": "^20.10.4",
        "@types/react": "^18.2.42",
        "@types/react-dom": "^18.2.17",
        "algoliasearch": "^4.22.1",
        "aos": "^3.0.0-beta.6",
        "clsx": "^2.1.0",
        "contentlayer2": "^0.4.6",
        "date-fns": "^2.30.0",
        "feed": "^4.2.2",
        "next": "^14.1.3",
        "next-contentlayer2": "^0.4.6",
        "next-plausible": "^3.12.0",
        "next-themes": "^0.2.1",
        "react": "18.2.0",
        "react-children-utilities": "^2.10.0",
        "react-dom": "18.2.0",
        "react-instantsearch-nextjs": "^0.1.14",
        "rehype-pretty-code": "^0.12.0",
        "rehype-slug": "^6.0.0",
        "remark-gfm": "^4.0.0",
        "schema-dts": "^1.1.2",
        "sharp": "^0.33.2",
        "shiki": "^0.14.5",
        "swiper": "^8.4.7",
        "typescript": "^5.3.3",
        "unist-util-visit": "^5.0.0"
      },
      "devDependencies": {
        "@tailwindcss/forms": "^0.5.7",
        "@tailwindcss/typography": "^0.5.10",
        "@types/aos": "^3.0.7",
        "autoprefixer": "^10.4.20",
        "postcss": "^8.4.32",
        "tailwindcss": "^3.4.1"
      }
